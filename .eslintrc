{
  "extends": "eslint-config-airbnb",
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6
  },
  "plugins": [
    "react"
  ],
  "env": {
    "browser": false,
    "es6": true
  },
  "rules": {
    "no-underscore-dangle": 0,
    "react/jsx-filename-extension": 0,
    "react/prop-types": 0,
    "object-curly-newline": 0,
    "linebreak-style": 0,
    "lines-between-class-members": 0
  },
  "globals": {
    "window": true,
    "document": true,
    "navigator": true,
    "fetch": true
  },
  "settings": {
    "import/resolver": {
      "babel-module": {
        "root": [
          "./src"
        ]
      }
    }
  }
}